    "gulp-stylus": "^2.5.0",
    "gulp-tslint": "^6.1.2",
    "gulp-typescript": "^3.0.2",
    "jasmine-core": "^2.5.2",
    "karma": "^1.3.0",
    "karma-coverage": "^1.1.1",
    "karma-jasmine": "^1.0.2",
    "karma-mocha-reporter": "^2.2.0",
    "karma-phantomjs-launcher": "^1.0.2",
    "karma-super-dots-reporter": "^0.1.0",
    "nib": "^1.1.2",
    "node-cmd": "^1.1.1",
